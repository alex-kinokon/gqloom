{
  "name": "@gqloom/zod",
  "version": "0.2.5",
  "description": "Create GraphQL schema and resolvers with TypeScript using Zod!",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "gqloom",
    "graphql",
    "schema",
    "typescript",
    "zod",
    "validation",
    "validate"
  ],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "graphql": ">= 16.8.0",
    "@gqloom/core": ">= 0.2.0",
    "zod": ">= 3.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@gqloom/core": "workspace:*",
    "zod": "^3.22.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
