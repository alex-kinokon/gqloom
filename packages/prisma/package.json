{
  "name": "@gqloom/prisma",
  "version": "0.0.1",
  "description": "GQLoom integration with Prisma",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gqloom",
    "graphql",
    "schema",
    "typescript",
    "prisma"
  ],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "graphql": ">= 16.8.0",
    "@gqloom/core": ">= 0.2.0",
    "@mikro-orm/core": ">= 6.0.0"
  },
  "devDependencies": {
    "@gqloom/core": "workspace:*",
    "@mikro-orm/better-sqlite": "^6.2.8",
    "@mikro-orm/core": "^6.2.8"
  },
  "homepage": "https://gqloom.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/modevol-com/gqloom.git",
    "directory": "packages/prisma"
  },
  "publishConfig": {
    "access": "public"
  }
}
