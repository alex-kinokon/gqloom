{
  "name": "@gqloom/prisma",
  "version": "0.1.1",
  "description": "GQLoom integration with Prisma",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./generated": {
      "import": {
        "types": "./generated/index.d.ts",
        "default": "./generated/index.js"
      },
      "require": {
        "types": "./generated/index.d.ts",
        "default": "./generated/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "release": "commit-and-tag-version",
    "init": "exec prisma migrate dev --name init",
    "generate": "prisma generate"
  },
  "bin": {
    "prisma-gqloom": "./bin/generator.cjs"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gqloom",
    "graphql",
    "schema",
    "typescript",
    "prisma"
  ],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "@gqloom/core": ">= 0.2.0",
    "@mikro-orm/core": ">= 6.0.0",
    "graphql": ">= 16.8.0"
  },
  "devDependencies": {
    "@gqloom/core": "workspace:*",
    "@gqloom/zod": "workspace:*",
    "@mikro-orm/better-sqlite": "^6.2.8",
    "@mikro-orm/core": "^6.2.8",
    "@prisma/client": "5.20.0",
    "graphql-yoga": "^5.6.0",
    "prisma": "^5.20.0",
    "zod": "^3.22.4"
  },
  "homepage": "https://gqloom.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/modevol-com/gqloom.git",
    "directory": "packages/prisma"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/generator-helper": "^5.20.0",
    "ts-morph": "^23.0.0"
  }
}
