{
  "name": "@gqloom/yup",
  "version": "0.1.1",
  "description": "Create GraphQL schema and resolvers with TypeScript using Yup!",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "xcfox",
  "license": "MIT",
  "peerDependencies": {
    "graphql": ">= 16.8.0",
    "@gqloom/core": ">= 0.1.0",
    "yup": ">= 1.0.0"
  },
  "devDependencies": {
    "yup": "^1.4.0",
    "@gqloom/core": "workspace:*"
  }
}
